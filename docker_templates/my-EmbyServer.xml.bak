<?xml version="1.0"?>
<Container version="2">
  <Name>EmbyServer</Name>
  <Repository>emby/embyserver:latest</Repository>
  <Registry>https://registry.hub.docker.com/u/emby/embyserver/</Registry>
  <Network>swag</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>&#xD;
    Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.[br][br]&#xD;
    [b][span style='color: #E80000;']Directories:[/span][/b][br]&#xD;
    [b]/config[/b] : this path is used to store Emby's configuration.[br]&#xD;
    [b]/mnt[/b] : this path is used to add library locations to Emby.[br]&#xD;
  </Overview>
  <Category>MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
  <WebUI>http://[IP]:[PORT:8096]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/MediaBrowser/Emby.Build/master/unraid-templates/emby/EmbyServer.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/MediaBrowser/Emby.Resources/master/images/Logos/logoicon.png</Icon>
  <ExtraParams>--device=/dev/dri</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1647953005</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>&#xD;
    Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.[br][br]&#xD;
    [b][span style='color: #E80000;']Directories:[/span][/b][br]&#xD;
    [b]/config[/b] : this path is used to store Emby's configuration.[br]&#xD;
    [b]/mnt[/b] : this path is used to add library locations to Emby.[br]&#xD;
  </Description>
  <Networking>
    <Mode>swag</Mode>
    <Publish>
      <Port>
        <HostPort>8096</HostPort>
        <ContainerPort>8096</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Downloads/</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/EmbyServer</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>UID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>GID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="http port" Target="8096" Default="8096" Mode="tcp" Description="http port" Type="Port" Display="always" Required="true" Mask="false">8096</Config>
  <Config Name="Media path" Target="/media" Default="" Mode="rw" Description="Container Path:/media" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/Downloads/</Config>
  <Config Name="Key 1" Target="UID" Default="2" Mode="" Description="Container Variable: UID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="Key 2" Target="GID" Default="2" Mode="" Description="Container Variable: GID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/EmbyServer" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/EmbyServer</Config>
</Container>
