<?xml version="1.0"?>
<Container version="2">
  <Name>EmbyServer</Name>
  <Repository>emby/embyserver:latest</Repository>
  <Registry>https://registry.hub.docker.com/u/emby/embyserver/</Registry>
  <Network>swag</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>&#xD;
    Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.&#xD;
&#xD;
&#xD;
    Directories:&#xD;
&#xD;
    /config : this path is used to store Emby's configuration.&#xD;
&#xD;
    /mnt : this path is used to add library locations to Emby.&#xD;
&#xD;
  </Overview>
  <Category>MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
  <WebUI>http://[IP]:[PORT:8096]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/MediaBrowser/Emby.Build/master/unraid-templates/emby/EmbyServer.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/MediaBrowser/Emby.Resources/master/images/Logos/logoicon.png</Icon>
  <ExtraParams>--device=/dev/dri</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1648301676</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="http port" Target="8096" Default="8096" Mode="tcp" Description="http port" Type="Port" Display="always" Required="true" Mask="false">8096</Config>
  <Config Name="Media path" Target="/media" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/Downloads/</Config>
  <Config Name="Key 1" Target="UID" Default="2" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="Key 2" Target="GID" Default="2" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/EmbyServer" Mode="rw" Description="" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/EmbyServer</Config>
</Container>
