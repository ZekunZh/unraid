<?xml version="1.0"?>
<Container version="2">
  <Name>qbittorrent-linuxserver</Name>
  <Repository>linuxserver/qbittorrent:latest</Repository>
  <Registry>https://github.com/orgs/linuxserver/packages/container/package/qbittorrent</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/linuxserver/docker-qbittorrent/issues/new/choose</Support>
  <Project>https://www.qbittorrent.org/</Project>
  <Overview>The Qbittorrent(https://www.qbittorrent.org/) project aims to provide an open-source software alternative to &#xB5;Torrent. qBittorrent is based on the Qt toolkit and libtorrent-rasterbar library.</Overview>
  <Category>Downloaders:</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/linuxserver/templates/master/unraid/qbittorrent.xml</TemplateURL>
  <Icon>https://gitee.com/jackiewuu/unraid/raw/master/icon/Qbittorrent.png</Icon>
  <ExtraParams>--cap-add=NET_ADMIN</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1679162349</DateInstalled>
  <DonateText>Donations</DonateText>
  <DonateLink>https://www.linuxserver.io/donate</DonateLink>
  <Requires/>
  <Config Name="网页界面 WebUI" Target="8080" Default="8080" Mode="tcp" Description="网页界面端口" Type="Port" Display="always" Required="true" Mask="false">8080</Config>
  <Config Name="BT下载端口 - TCP" Target="6881" Default="6881" Mode="tcp" Description="TCP BT 下载端口 " Type="Port" Display="always" Required="true" Mask="false">6881</Config>
  <Config Name="BT下载端口 - UDP" Target="6881" Default="6881" Mode="udp" Description="UDP BT 下载端口" Type="Port" Display="always" Required="true" Mask="false">6881</Config>
  <Config Name="下载路径" Target="/downloads" Default="" Mode="rw" Description="下载路径映射" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/</Config>
  <Config Name="网页端口变量" Target="WEBUI_PORT" Default="8080" Mode="{3}" Description="网页端口变量，需要与上面的网页端口号一致" Type="Variable" Display="always" Required="true" Mask="false">8080</Config>
  <Config Name="配置文件" Target="/config" Default="/mnt/user/appdata/qbittorrent" Mode="rw" Description="QB配置文件保存路径，使用这里的默认值即可" Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata/qbittorrent</Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">100</Config>
  <Config Name="UMASK" Target="UMASK" Default="022" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">002</Config>
</Container>