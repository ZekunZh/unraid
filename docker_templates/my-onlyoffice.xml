<?xml version="1.0"?>
<Container version="2">
  <Name>onlyoffice</Name>
  <Repository>onlyoffice/documentserver</Repository>
  <Registry>https://hub.docker.com/r/onlyoffice/documentserver/</Registry>
  <Network>swag</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/75588-support-siwats-docker-repository/</Support>
  <Project>https://www.onlyoffice.com/</Project>
  <Overview>ONLYOFFICE Document Server is an online office suite comprising viewers and editors for texts, spreadsheets and presentations, fully compatible with Office Open XML formats: .docx, .xlsx, .pptx and enabling collaborative editing in real time.&#xD;
	HTTPS Support can be enable creating a "certs" folder in the data directory and placing onlyoffice.crt and onlyoffice.key in it. Add fonts by placing .ttf in the font folder and force updating the container.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:9090]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/SiwatINC/unraid-ca-repository/master/docker-template/onlyoffice_document_server.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/SiwatINC/unraid-ca-repository/master/icons/onlyoffice.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1662995727</DateInstalled>
  <DonateText>If you like it, then consider buying me a soda as I don&amp;amp;#39;t like beer :P</DonateText>
  <DonateLink>https://www.paypal.me/siwatsirichai</DonateLink>
  <Requires/>
  <Config Name="Host Port 1" Target="80" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">9090</Config>
  <Config Name="Host Port 2" Target="443" Default="4430" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">4430</Config>
  <Config Name="Host Path 1" Target="/var/log/onlyoffice" Default="/mnt/user/appdata/onlyofficeds/logs" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyofficeds/logs</Config>
  <Config Name="Host Path 2" Target="/var/www/onlyoffice/Data" Default="/mnt/user/appdata/onlyofficeds/Data" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyofficeds/Data</Config>
  <Config Name="Host Path 3" Target="/var/lib/onlyoffice" Default="/mnt/user/appdata/onlyofficeds/lib" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyofficeds/lib</Config>
  <Config Name="Host Path 4" Target="/var/lib/postgresql" Default="/mnt/user/appdata/onlyofficeds/database" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyofficeds/database</Config>
  <Config Name="Host Path 5" Target="/usr/share/fonts/" Default="/mnt/user/appdata/onlyofficeds/fonts" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyofficeds/fonts</Config>
  <Config Name="Enable Traefik" Target="traefik.enable" Default="true" Mode="" Description="" Type="Label" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="traefik https" Target="traefik.http.routers.onlyoffice.entryPoints" Default="https" Mode="" Description="" Type="Label" Display="always" Required="false" Mask="false">https</Config>
  <Config Name="GUID" Target="GUID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
</Container>
